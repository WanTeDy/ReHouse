    @if (ViewBag.AdminFeedbacks != null && ViewBag.AdminFeedbacks.Count > 0)
    {
        <script type="text/javascript">
            $(function () {
                var list = [], count = @ViewBag.AdminFeedbacks.Count;
                for(var i = 1; i<= count; i++) {
                    list.push(".f" + i);
                }
                $('#testimonials').jCarouselLite({
                    vertical: false,
                    hoverPause: true,
                    btnPrev: ".f-previous",
                    btnNext: ".f-next",
                    visible: 1,
                    start: 0,
                    scroll: 1,
                    circular: true,
                    auto: 6000,
                    speed: 900,
                    //autoCSS: false,
                    autoWidth: true,
                    responsive: true,
                    btnGo: list,

                    afterEnd: function (a, to, btnGo) {
                        if (btnGo.length <= to) {
                            to = 0;
                        }
                        $(".thumbActive").removeClass("thumbActive");
                        $(btnGo[to]).addClass("thumbActive");
                    }
                });
            });
        </script>
    }
    <script type="text/javascript">
